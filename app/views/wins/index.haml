-@wins.each do |w|
  %table.win
    %tr.status
      %td.profile_picture{:valign=>"top"}
        =image_tag w.user.photo.url(:thumb)
      %td.detail
        %span.username
          %a{:href=>"/users/#{w.user.username}", :class=>"username"}
            =w.user.username
        =render :partial => "shared/status", :locals => {:w=>w}